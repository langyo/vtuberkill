{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "importHelpers": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "lib": ["DOM", "es6", "webworker"],
    "target": "ESNext"
  },
  "include": ["src/**/*"]
}
